{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "no-console": false,
        "interface-over-type-literal": false,
        "max-line-length": false,
        "interface-name": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}